{
  "name": "bumbag",
  "version": "0.0.0-6",
  "description": "An accessible, composable, and friendly React UI Kit",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "yarn build:lib && yarn build:docs",
    "build:lib": "nwb build-react-component",
    "build:docs": "docz build",
    "clean": "nwb clean-module",
    "develop": "nodemon -w src -x 'nwb build-react-component'",
    "prepublish": "yarn build",
    "flow": "flow",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test": "yarn flow && yarn lint && jest",
    "docs": "docz dev"
  },
  "dependencies": {
    "classnames": "2.2.6",
    "lodash": "4.17.11",
    "polished": "2.1.1",
    "reakit": "0.15.3",
    "styled-tools": "1.5.1"
  },
  "peerDependencies": {
    "react": "16.x",
    "react-dom": "16.x"
  },
  "devDependencies": {
    "@medipass/eslint-config-react-medipass": "8.3.6",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "docz": "0.11.2",
    "docz-theme-default": "0.11.2",
    "eslint": "5.6.0",
    "flow-bin": "0.81.0",
    "jest": "23.6.0",
    "jest-styled-components": "6.2.1",
    "nodemon": "1.18.4",
    "nwb": "0.23.0",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-testing-library": "5.1.0"
  },
  "author": "Jake Moxey & Dave Olsen",
  "homepage": "https://github.com/JakeNDavesCompanyPtyLtdEnterprises",
  "license": "MIT",
  "repository": "JakeNDavesCompanyPtyLtdEnterprises/bumbag",
  "keywords": [
    "bumbag",
    "ui",
    "accessible",
    "composable"
  ],
  "jest": {
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    }
  }
}
