    "react": "16.x"
  },
  "devDependencies": {
    "@medipass/eslint-config-react-medipass": "8.3.6",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "docz": "0.11.1",
    "eslint": "5.5.0",
    "jest": "23.6.0",
    "jest-styled-components": "6.2.0",
    "nodemon": "1.18.4",
    "nwb": "0.23.0",
    "prop-types": "15.6.2",
    "react": "16.5.0",
    "react-dom": "16.5.0",
    "react-testing-library": "5.1.0"
